public class LogInBean {

    /**
     * 登录凭证
     */
    private String access_token;
    private User user;

    /**获取登录凭证
     * @return
     */
    public String getAccess_token() {
        return access_token;
    }

    public void setAccess_token(String access_token) {
        this.access_token = access_token;
    }

    public User getUser() {
        return user;
    }

    public void setUser(User user) {
        this.user = user;
    }
}
